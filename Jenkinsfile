pipeline {
    agent any

    stages {
        stage('hello') {
            steps {
                echo 'hello world'
            }

         }
	    stage('build') {
            steps {
                echo 'hello build'
            }

         }
	    stage('deploy') {
            steps {
                echo 'hello deploy '
            }

         }
	    stage('test') {
            steps {
                echo 'hello test'
            }

         }

     }
}
